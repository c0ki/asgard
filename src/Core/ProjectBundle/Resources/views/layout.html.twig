{% extends "CoreAdminBundle::layout.html.twig" %}

{% block head_css %}
    {{ parent() }}
    {% stylesheets
    'bundles/coreproject/css/default.css'
    filter='cssrewrite' %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}

{% block header_navbar %}
    {% include 'CoreProjectBundle:Default:projects_list.html.twig' with {'route': 'core_project_project'} only %}
    {{ parent() }}
{% endblock %}

{% block tools_navbar %}
    {% include 'CoreProjectBundle:Default:domains_list.html.twig' with {'select': true} only %}
    {{ parent() }}
{% endblock %}