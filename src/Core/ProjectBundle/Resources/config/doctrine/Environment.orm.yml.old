Core\ProjectBundle\Entity\Environment:
    type: entity
    table: environment
    id:
        id:
            type: integer
            generator: { strategy: AUTO }
    fields:
        name:
            type: string
            length: 20
        label:
            type: string
            length: 50
        description:
            type: text
        logo:
            type: string
            length: 20
            nullable: true

    manyToOne:
        project:
            targetEntity: Core\ProjectBundle\Entity\Project
            inversedBy: environments
            joinColumn:
                name: project_id
                referencedColumnName: id